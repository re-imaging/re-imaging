          {% if not embedding %}
            {% set search_mode_text = "results by sampling dataset" %}
          {% else %}
            {% set search_mode_text = "nearest results to the selected image by {} embedding".format(embedding) %}
          {% endif %}
          {{ "Showing {} {}".format(images_shown, search_mode_text) }}
          </div>
